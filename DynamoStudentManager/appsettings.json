{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentUserName",
      "WithAssemblyName",
      "WithAssemblyVersion"
    ],
    "Properties": {
      "ApplicationName": "DynamoStudentManager"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Properties}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://elasticsearch-1.lucasnet.int:9200",
          "indexFormat": "dynamo-logs-{0:yyyy.MM}",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv7"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/DynamoStudentManager_.log",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://elasticsearch-1.lucasnet.int:9200"
  },
  "AllowedHosts": "*",
  "AWS": {
    "Profile": "default",
    "ServiceURL": "http://dynamodb-1.lucasnet.int:8000"
  }
}
